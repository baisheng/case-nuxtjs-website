<script type="text/javascript">

  $(function() {

    $.getJSON("/tours/get/active", function(data) {

      $.each(data.response, function(){

        var tour_date = this.tour_date.split(" ");
        var tour_date_without_time = tour_date[0].split("-");
        var tour_date_month = tour_date_without_time[1];
        var tour_date_day = tour_date_without_time[2];

        $('.modal.tour table').append('<tr><td><p>' + tour_date_month + '.' + tour_date_day + '</p></td><td><p>' + this.tour_city + '</p><p class="mobile">' + this.tour_copy + '</p></td><td><p>' + this.tour_copy + '</p></td><td><a href="' + this.tour_url + '" class="button" target="_blank"><span class="desktop">Get tickets</span><span class="mobile">Buy</span></a></td></tr>');

      });

        console.log("done.....");
